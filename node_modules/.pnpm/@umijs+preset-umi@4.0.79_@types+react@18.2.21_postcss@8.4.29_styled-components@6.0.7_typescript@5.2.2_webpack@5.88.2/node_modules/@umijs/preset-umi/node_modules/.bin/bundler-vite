#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/bundler-vite/bin/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/bundler-vite/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/bundler-vite/bin/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/bundler-vite/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules:/Users/gaoxin/Documents/项目/umi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/bundler-vite/bin/bundler-vite.js" "$@"
else
  exec node  "$basedir/../../../../../../@umijs+bundler-vite@4.0.79_postcss@8.4.29/node_modules/@umijs/bundler-vite/bin/bundler-vite.js" "$@"
fi
